{
  "name": "smt-lsp",
  "displayName": "SMT LSP",
  "description": "A Language Server Protocol for the SMT-LIB Standard",
  "version": "1.1.0",
  "publisher": "hra687261",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/hra687261/smt-lsp-vscode-extension"
  },
  "categories": [
    "Programming Languages"
  ],
  "engines": {
    "vscode": "^1.86.0"
  },
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "SMT LSP",
      "properties": {
        "smt-lsp.preludes": {
          "type": "array",
          "items": "string",
          "default": [],
          "description": "Prelude files that the language server processes before the files that are open. The files are processed in the order in which they appear."
        },
        "smt-lsp.binary": {
          "type": "string",
          "default": null,
          "description": "Path to the dolmenls (Dolmen language server) binary (likely under \"~/.opam/{opam-switch}/bin/dolmenls\" or equivalent)"
        }
      }
    },
    "languages": [
      {
        "id": "smt",
        "aliases": [
          "SMT"
        ],
        "extensions": [
          ".smt",
          ".smt2",
          ".psmt2"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "smt",
        "scopeName": "source.smt",
        "path": "./smt.tmLanguage"
      }
    ],
    "comments": {
      "lineComment": ";"
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "vscode-languageclient": "^8.0.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.11.18",
    "@types/vscode": "^1.86.0",
    "eslint": "^8.56.0",
    "typescript": "^4.9.4"
  }
}