{
  "name": "smt-lsp",
  "displayName": "SMT LSP",
  "description": "A Language Server Protocol for the SMT-LIB Standard",
  "version": "0.0.1",
  "publisher": "hra687261",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/hra687261/vscode-smt-lsp"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:smt"
  ],
  "engines": {
    "vscode": "^1.73.1"
  },
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "SMT LSP",
      "properties": {
        "smt-lsp.preludes": {
          "type": "array",
          "default": [],
          "description": "Prelude files that the language server processes first."
        }
      }
    },
    "languages": [
      {
        "id": "smt",
        "aliases": [
          "SMT"
        ],
        "extensions": [
          ".smt",
          ".smt2",
          ".psmt2"
        ]
      }
    ],
    "grammars": [
      {
        "language": "smt",
        "scopeName": "source.smt",
        "path": "./smt.tmLanguage"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "vscode-languageclient": "^8.0.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@types/node": "18.11.9",
    "@types/vscode": "^1.73.1",
    "eslint": "^6.1.3",
    "typescript": "4.9.3"
  }
}